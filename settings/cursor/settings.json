{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "workbench.statusBar.feedback.visible": false,
    "workbench.editor.showTabs": "single",
    "explorer.openEditors.visible": 0,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.currentLine.enabled": false,
    "editor.scrollBeyondLastLine": false,
    "window.title": "${rootName}",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "python.autoComplete.addBrackets": true,
    "terraform.format": {
        "enable": true,
        "ignoreExtensionsOnSave": [
            ".tfsmurf"
        ],
        "formatOnSave": false
    },
    "gitlens.historyExplorer.enabled": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb",
        "git-rebase-todo": "default"
    },
    "plantuml.exportFormat": "png",
    "plantuml.exportOutDir": "~/Downloads",
    "window.zoomLevel": 1,
    "terminal.integrated.copyOnSelection": true,
    "python.analysis.completeFunctionParens": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "extensions.ignoreRecommendations": true,
    "editor.inlineSuggest.enabled": true,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "go.toolsManagement.autoUpdate": true,
    "git.openRepositoryInParentFolders": "never",
    "editor.minimap.enabled": false,
    "python.languageServer": "None",
    "git.confirmSync": false,
    "explorer.confirmDelete": false,
    "githubPullRequests.pullBranch": "never",
    "github.copilot.editor.enableAutoCompletions": true,
    "search.mode": "newEditor",
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.statusBar.pullRequests.enabled": false,
    "gitlens.hovers.pullRequests.enabled": false,
    "gitlens.graph.pullRequests.enabled": false,
    "gitlens.views.commits.pullRequests.enabled": false,
    "gitlens.views.commits.pullRequests.showForCommits": false,
    "gitlens.views.commitDetails.pullRequests.enabled": false,
    "gitlens.launchpad.indicator.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "cursor-retrieval.canAttemptGithubLogin": false,
    "workbench.colorTheme": "GitHub Dark",
    "redhat.telemetry.enabled": false,
    "biome.suggestInstallingGlobally": false,
    "cursorpyright.shouldImportPylanceSettings": "never",
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "python.createEnvironment.trigger": "off",
    "workbench.editor.enablePreview": false
}